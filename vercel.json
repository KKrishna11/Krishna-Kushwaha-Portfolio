{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "upgrade-insecure-requests"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/",
            "destination": "/index.html",
            "permanent": true
        }
    ]
}